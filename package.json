{
  "name": "lp-site",
  "version": "1.0.0",
  "description": "flat-file blog starter",
  "main": "index.js",
  "scripts": {
    "build": "gatsby build",
    "check-format": "npm run pretty -- --list-different",
    "clean": "rm -rf dist && gatsby clean",
    "commit": "npx git-cz",
    "dev": "npx browserslist@latest --update-db && gatsby develop",
    "format": "npm run pretty -- --write",
    "lint": "eslint --ignore-path .gitignore --ext .js --ext .jsx .",
    "pretty": "prettier --ignore-path .gitignore \"**/*.+(js|json|md)\"",
    "serve": "gatsby serve",
    "test": "jest",
    "coverage": "jest --config jest.config.js",
    "snapshot": "npm test -- -u",
    "update-tests": "jest -- -u",
    "validate": "npm-run-all --parallel check-format lint",
    "build:prefix": "gatsby build --prefix-paths",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public -r https://$GH_TOKEN@github.com/Loonz206/lp-site.git",
    "prepare": "npm run snyk-protect",
    "snyk-protect": "snyk-protect",
    "cy:run": "npx cypress run",
    "cy:open": "npx cypress open",
    "e2e:test": "start-server-and-test dev http://localhost:8000 cy:run",
    "e2e:open": "start-server-and-test dev http://localhost:8000 cy:open"
  },
  "keywords": [
    "gatsby"
  ],
  "author": "loonz206 <@loonz206>",
  "license": "ISC",
  "dependencies": {
    "@snyk/protect": "^1.1091.0",
    "@testing-library/cypress": "^8.0.3",
    "babel-jest": "^24.9.0",
    "cypress": "^10.3.0",
    "dotenv": "^8.2.0",
    "gatsby": "^2.31.0",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sass": "^2.8.0",
    "gatsby-source-filesystem": "^2.10.0",
    "gatsby-transformer-remark": "^2.15.0",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-redux": "^7.2.2",
    "redux": "^4.0.4",
    "sass": "^1.53.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.8.3",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@testing-library/jest-dom": "^5.0.0",
    "@testing-library/react": "^10.0.0",
    "babel-preset-gatsby": "^0.2.26",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sonarjs": "^0.10.0",
    "expect": "^25.1.0",
    "gh-pages": "^2.2.0",
    "husky": "^4.2.3",
    "identity-obj-proxy": "^3.0.0",
    "istanbul": "^0.4.5",
    "jest": "^24.9.0",
    "lint-staged": "^10.0.8",
    "mocha": "^7.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.12.0",
    "start-server-and-test": "^1.14.0"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
  "snyk": true
}
